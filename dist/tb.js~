    var head = document.querySelector( 'head' );
    if ( head ) {
        head.appendChild( __css );
    } else {
        setTimeout( arguments.callee, 20 );
    }
}, 20 );
(function ( w, undefined ) {

    var PatternReplace = $tb.Replacer = function ( opts ) {
        this.opts = opts;
        this.re = [
            /[\[\]А-Яа-яA-Za-z0-9\u200b\.\/:\-_#]*?[\/|:|%|&]([А-Яа-яA-Za-z0-9\-_]+?)[\/|:|%|&]/,
            //vkplugin smiles
            /\[\s[\.а-я-А-Яa-zA-Z:\u200b0-9\/]+?#смайл:([а-я-А-Яa-zA-Z0-9]+?)\s\]/,
            /[\.а-я-А-Яa-zA-Z:\u200b0-9\/]+?#смайл:([а-я-А-Яa-zA-Z0-9]+)/
        ];
    };

    PatternReplace.prototype.test = function ( s ) {
        return this.re.exec( s );
    };

    PatternReplace.prototype.replace = function ( el ) {
        var tnodes = $tb.utils._findAllTextNodes( el, ['TEXTAREA'] ),
            smile,
            sid,
            config = this.opts.config.parsed,
            newNode,
            i,
            l = this.re.length,
            curNode,
            match;

        while ( curNode = tnodes.shift() ) {
            for ( var j = 0; j < l; j++ ) {
                match = this.re[j].exec( curNode.nodeValue );
                if ( match ) {
                    sid = match[1];
                    for ( i in config ) {
                        if ( i === sid || ~config[i].alias.indexOf( sid ) ) {
                            curNode.nodeValue = curNode.nodeValue.replace( match[0], '' );
                            newNode = curNode.splitText( match.index );
                            smile = document.createElement( 'img' );
                            smile.setAttribute( 'src', config[i].url );
                            newNode.parentNode.insertBefore( smile, newNode );
                            tnodes.push( newNode );
                            break;
                        }
                    }
                }
            }
        }
    };

    var DomChecker = $tb.Checker = function ( config ) {
        this.config = config;
        this.query = {};
    };

    DomChecker.prototype.addSelector = function ( selector, fn ) {
        this.query[selector] = fn;
    };

    DomChecker.prototype.check = function () {
        var self = this;
        setInterval( function () {
            for ( var q in self.query ) {
                var els = document.querySelectorAll( q );

                for ( var i = 0, l = els.length; i < l; i++ ) {
                    if ( els[i].hasAttribute( 'trollbar-checked' ) ) continue;
                    var opts = self.query[q]( els[i], {} );
                    if ( opts ) {
                        els[i].setAttribute( 'trollbar-checked', true );
                    } else {
                        continue;
                    }
                    opts = $tb.utils.extend( {
                        iframe        : undefined,
                        el            : undefined,
                        dir           : undefined,
                        insertBeforeEl: els[i],
                        textEl        : els[i],
                        config        : self.config
                    }, opts );

                    new $tb.Bar( opts ).render();
                }
            }
//            var els = document.querySelectorAll( self.queryString ),
//                qk = this.queryKeys,
//                id,
//                fnIndex,
//                opts = {};
//
//            for ( var i = 0, el = els.length; i < el; i++ ) {
//                id = '#' + els[i].id;
//                for ( var j = 0, ql = qk.length; j < ql; j++ ) {
//                    if ( $tb.utils._hasClass( els[i], qk[j] ) || ) {
//
//                    }
//                }
//
//            }

        }, 100 );
    };

})( window );
(function ( w, undefined ) {
    var Popup = $tb.Popup = function ( opts ) {
        this.opts = opts;
        this.wrapper = null;
        this.content = null;
        this.corner = null;
    };

    Popup.prototype.resources = {
        preloader: 'data:image/gif;base64,'
    };

    Popup.prototype.init = function () {
        var wrapper;
        var self = this;
        var corner = this.corner = document.createElement( 'div' );
        corner.className = 'trollbar-popup-corner';
        this.wrapper = wrapper = document.createElement( 'div' );
        this.content = document.createElement( 'div' );
        this.content.className = 'trollbar-popup-content';
        wrapper.className = 'trollbar-popup';
        wrapper.style.position = this.opts.position || 'absolute';
        wrapper.appendChild( corner );
        var closeBtn = document.createElement( 'div' );
        closeBtn.className = 'close-btn';
        closeBtn.addEventListener( 'mousedown', function ( e ) {
            e.preventDefault();
            e.stopPropagation();
            self.hide();
        } );
        wrapper.appendChild( closeBtn );
    };

    Popup.prototype.preloader = function () {
        //TODO move to css
        return "<img src='" + this.resources.preloader + "' style='position: absolute; top: 50%; left: 50%; margin-top: -8px; margin-left: -8px;'/>";

    };

    Popup.prototype.hide = function () {
        this.wrapper.classList.remove( 'active' );
        this.wrapper.style.display = 'none';
    };

    Popup.prototype.show = function ( target, style ) {
        this.wrapper.style.display = 'block';

        this.render( target, {
            width : 315,
            height: 255
        } );

        for ( var i in style ) {
            this.wrapper.style[i] = style[i];
        }
        this.wrapper.classList.add( 'active' );
    };

    Popup.prototype.drawCorner = function ( pos ) {
        this.corner.style.top = '';
        this.corner.style.left = '';
        this.corner.style.right = '';
        this.corner.style.bottom = '';
        this.corner.style.margin = '';

        this.corner.classList.add( pos );
    };

    Popup.prototype.pos = function ( left, top ) {
        this.wrapper.style.top = top + 'px';
        this.wrapper.style.left = left + 'px';
    };

    Popup.prototype.render = function ( target, res ) {
        var offset = $tb.utils._getOffsetRect( target ),
            targetWidth = target.offsetWidth,
            targetHeight = target.offsetHeight,
            wLeft,
            wTop,
            pad = 30,
            windowSize = $tb.utils._windowSize(),
            scroll = $tb.utils._scroll();

        if ( res.html ) {
            this.content.innerHTML = res.html;
        }
        this.wrapper.style.width = res.width + 'px';
        this.wrapper.style.height = res.height + 'px';
        this.wrapper.appendChild( this.content );

        var draw = {
            s : function () {
                wTop = pad + targetHeight;
                wLeft = targetWidth / 2 - res.width / 2;
                this.drawCorner( 'top' );
                this.pos( wLeft, wTop );
            },
            se: function () {

            },
            w : function () {
                wLeft = -pad - res.width;
                wTop = -res.height / 2 + targetHeight / 2;
                this.drawCorner( 'right' );
                this.pos( wLeft, wTop );
            },
            n : function () {
                wTop = -pad - res.height;
                wLeft = +targetWidth / 2 - res.width / 2;
                this.drawCorner( 'bottom' );
                this.pos( wLeft, wTop );
            },
            ne: function () {
                wTop = -pad - res.height;
                wLeft = 0;
//                this.drawCorner( 'bottom' );
                this.pos( wLeft, wTop );
            },
            nw: function () {
                wTop = -pad - res.height;
                wLeft = -res.width;
//                this.drawCorner( 'bottom' );
                this.pos( wLeft, wTop );
            },
            e : function () {
                wLeft = targetWidth + pad;
                wTop = res.height / 2 + targetHeight / 2;
                this.drawCorner( 'left' );
                this.pos( wLeft, wTop );
            }
        };

        console.log( offset, targetWidth, targetHeight, res.width, res.height );

        if ( this.opts.dir ) {
            console.log( 'predefined dir', this.opts.dir );
            return draw[this.opts.dir].bind( this )();
        } else {
            //bottom side
            if ( offset.top + pad + res.height < scroll.top + windowSize.height &&
                ( offset.left + targetWidth / 2 - res.width / 2 > scroll.left &&
                    offset.left + targetWidth / 2 + res.width / 2 < scroll.left + windowSize.width) ) {
                return draw.s.bind( this )();
            }
            //left side
            if ( offset.left - pad - res.width > scroll.left &&
                ( offset.top + targetHeight / 2 - res.height / 2 > scroll.top &&
                    offset.top + targetHeight / 2 + res.height / 2 < scroll.top + windowSize.height )
                ) {
                return draw.w.bind( this )();
            }
            //right side
            if ( offset.left + targetWidth + pad + res.width < scroll.left + windowSize.width &&
                ( offset.top + targetHeight / 2 - res.height / 2 > scroll.top &&
                    offset.top + targetHeight / 2 + res.height / 2 < scroll.top + windowSize.height )
                ) {
                return draw.e.bind( this )();
            }
            //top side
            if ( this.opts.dir == 'top' || offset.top - pad - res.height > scroll.top &&
                ( offset.left + targetWidth / 2 - res.width / 2 > scroll.left &&
                    offset.left + targetWidth / 2 + res.width / 2 < scroll.left + windowSize.width) ) {
                return draw.n.bind( this )();
            }
        }

        console.log( wLeft, wTop );

    };
})( window );

(function ( w, undefined ) {
    /**
     *
     * @param opts
     * @param opts.insertBeforeEl Dom node
     * @param opts.textEl Dom node
     * @param opts.config Object
     */
    var TrollBar = $tb.Bar = function ( opts ) {
        this.opts = opts;
        this.config = opts.config;
        this.container = null;
        this.tabs = {};
        this.popup = new $tb.Popup( {
            dir: opts.dir
        } );
        this.popup.init();
        this.popupContainer = $tb.utils._el( 'div' );
        this.popupContainer.className = 'trollbar-popup-panel-wrapper';
        this.generateTabs( this.config );
        this.generateOptionsTab();
//        this.generateInfoTab();
        this.popup.content.appendChild( this.popupContainer );
        this.switchTab( this.popup.content.querySelector( '.trollbar-tab' ) );

        //TODO
        if ( opts.panelStyle ) {
            this.opts.btnStyle = this.opts.btnStyle || {};
            this.container = this.generateBar( this.config );
            this.popupShowBtn = this.container.lastChild;
        }
        else if ( opts.el ) {
            this.popupShowBtn = opts.el;
            this.container = opts.el;
        }
//        else {
//            this.container = document.createElement( opts.tag || 'div' );
//            this.showPopupLink = document.createElement( 'a' );
//            this.showPopupLink.innerHTML = 'trollbar';
//            this.container.style.display = 'inline-block';
//            this.container.style.position = 'relative';
//            if ( opts.style ) {
//                for ( var i in opts.style ) {
//                    this.container.style[i] = opts.style[i];
//                }
//            }
//            this.container.appendChild( this.showPopupLink );
//        }
        this.popupShowBtn.classList.add( 'trollbar-popup-btn-show' );
        this.popupShowBtn.appendChild( this.popup.wrapper );
        this.bind();
    };

    TrollBar.prototype.serialize = function ( el ) {
        var map = {};
        var inputs = el.querySelectorAll( 'input' );
        for ( var i = 0; i < inputs.length; i++ ) {
            var id = inputs[i].getAttribute( 'data-option-id' );
            var type = inputs[i].getAttribute( 'type' );
            if ( type == 'checkbox' ) {
                map[id] = {
                    v: inputs[i].checked
                };
            }
        }
        return map;
    };

    TrollBar.prototype.togglePanel = function ( el ) {
        var panelOffset = el.offsetLeft;
        var containerOffset = parseInt( this.popupContainer.style.left, 10 ) || 0;
        this.popupContainer.style.left = -panelOffset == containerOffset ? '0' : -panelOffset + 'px';
    };

    TrollBar.prototype.generateInfoTab = function () {
        var panel = this.panelEl( {header: 'О плагине'} );
        var infoBtn = $tb.utils._el( 'div' );
        var self = this;
        infoBtn.className = 'trollbar-popup-panel-btn trollbar-info-btn';
        infoBtn.addEventListener( 'click', function () {
            self.togglePanel( panel );
        } );
        var info = $tb.utils._el( 'div' );
        info.innerHTML = '<div class="trollbar-option-row"> <a target="_blank" href="http://trollbar.net"/>Официальный сайт трольбара</a></div>' +
            '<div class="trollbar-option-row"> <a target="_blank" href="http://vk.com/club43281004"/>Группа в VK</a></div>' +
            '<div class="trollbar-option-row"> <a target="_blank" href="http://trollbar.net/faq"/>Часто задаваемые вопросы (FAQ)</a></div>'+
            '<div class="trollbar-option-row"> <a target="_blank" href="http://vksaverplus.ru"/>VKSaver+ ( скачивание видео и аудио для VK)</a></div>'+
            '<div class="trollbar-option-row"> <a target="_blank" href="http://vk.com/club43281769"/>Группа VKSaver+</a></div>';
        panel.appendChild( info );
        this.popup.wrapper.appendChild( infoBtn );
    };

    TrollBar.prototype.panelEl = function ( opts ) {
        var panel = $tb.utils._el( 'div', {} );
        panel.innerHTML = "<h2 class='trollbar-panel-header'>" + opts.header + "</h2>";
        panel.className = 'trollbar-popup-panel';
        this.popupContainer.appendChild( panel );
        return panel;
    };

    TrollBar.prototype.generateOptionsTab = function () {
        var optsBtn = $tb.utils._el( 'div' );
        var self = this;
        optsBtn.className = 'trollbar-popup-panel-btn trollbar-options-btn';
        optsBtn.addEventListener( 'click', function () {
            self.togglePanel( panel );
        } );

        this.popup.wrapper.appendChild( optsBtn );
        var panel = this.panelEl( {header: 'Настройки'} );

        var options = $tb.storage.get( 'options' );
        var optionRow = $tb.utils._el( 'div' );
        optionRow.classList.add( 'trollbar-option-row' );
        for ( var prop in options ) {
            if ( typeof options[prop].v === 'boolean' ) {
                var cb = $tb.utils._el( 'input' );
                cb.setAttribute( 'data-option-type', 'boolean' );
                cb.setAttribute( 'data-option-id', prop );
                cb.checked = options[prop].v;
                cb.type = 'checkbox';
                var label = $tb.utils._el( 'label' );
                label.innerHTML = options[prop].desc;
                label.insertBefore( cb, label.firstChild );
//                optionRow.appendChild( cb );
                optionRow.appendChild( label );
                panel.appendChild( optionRow );
            }
        }
        panel.addEventListener( 'change', function ( e ) {
            $tb.storage.set( {options: self.serialize( panel ) } );
        } );
        return panel;
    };

    TrollBar.prototype.generateBar = function ( config ) {
        if ( config.smiles.__bar && Object.keys( config.smiles.__bar ).length ) {

            var panel = $tb.utils._el( 'div', this.opts.panelStyle );
            panel.className = 'trollbar-bar';
            var smilesWrapper = $tb.utils._el( 'div', {
                cssFloat: 'left',
                overflow: 'hidden',
                zIndex  : 0
            } );
            var btnStyle = $tb.utils.extend( {
                zIndex     : '0',
                position   : 'relative',
                cssFloat   : 'right',
                marginRight: '5px',
                marginTop  : '5px'
            }, this.opts.btnStyle );

            var tbBtn = $tb.main.defaultBtnEl( btnStyle );
            var bar = config.smiles.__bar;
            for ( var j in bar ) {
                var smileOpts = bar[j];
                smileOpts.size = [28, 28];
                smilesWrapper.appendChild( this.smileEl( smileOpts ) );
            }
            panel.appendChild( smilesWrapper );
            panel.appendChild( tbBtn );
        }
        return panel;
    };

    /**
     *
     * @param opts {Object}
     * @param opts.size {Array}
     * @param opts.id {String}
     * @param opts.url {String}
     * @return {Element}
     */
    TrollBar.prototype.smileEl = function ( opts ) {
        var smile = document.createElement( 'a' );
        var style = opts.size ?
            'style="width: ' + opts.size[0] + 'px; max-height:' + opts.size[1] + 'px; "' : '';

        smile.style.cursor = 'pointer';
        smile.innerHTML = '<img class="tb-smile" data-smile-id="' + opts.id + '"' + style + 'src="' + opts.url + '"/>';
        return smile;
    };

    /**
     *
     * @param config
     * @return {Element}
     */
    TrollBar.prototype.generateTabs = function ( config ) {
        var groups = config.smiles;
        var panel = this.panelEl( {header: 'Смайлы'} );

        var tabWrapper = document.createElement( 'div' );
        tabWrapper.className = 'trollbar-tab-wrapper';
        panel.appendChild( tabWrapper );

        for ( var i in groups ) {
            if ( i == '__hidden' || i == '__bar' ) continue;
            var tab = document.createElement( 'div' );
            tab.innerHTML = i;
            tab.className = 'trollbar-tab';
            tab.setAttribute( 'data-tab-id', i );
            tab.addEventListener( 'mousedown', this.onTabClick.bind( this ) );
            var smileSize = config.size[i] ? config.size[i].split( 'x' ) : undefined;
            var smileContainer = document.createElement( 'div' );
            smileContainer.className = 'trollbar-tab-container';
            smileContainer.style.textAlign = 'left';
            smileContainer.style.clear = 'both';
            smileContainer.style.lineHeight = '1';
            smileContainer.style.display = 'none';
            smileContainer.setAttribute( 'data-tab-id', i );

            for ( var j in groups[i] ) {
                var smileOpts = groups[i][j];
                smileOpts.size = smileSize;
                smileContainer.appendChild( this.smileEl( smileOpts ) );
            }
            tabWrapper.appendChild( tab );
            panel.appendChild( smileContainer );
        }

        return panel;
    };

    TrollBar.prototype.onTabClick = function ( e ) {
        this.switchTab( e.target );
        e.preventDefault();
        e.stopPropagation();
    };

    TrollBar.prototype.bind = function () {
        var self = this;
        this.popupShowBtn.addEventListener( 'mousedown', function ( e ) {
            self.popup.show( self.popupShowBtn, self.opts.popupStyle );
        } );

        this.container.addEventListener( 'click', function ( e ) {
            if ( $tb.utils._hasClass( e.target, "trollbar-popup-panel-btn" ) ) {
                var btns = self.popup.wrapper.querySelectorAll( '.trollbar-popup-panel-btn' );
                var addClass = !$tb.utils._hasClass( e.target, 'active' );
                for ( var i = 0; i < btns.length; i++ ) {
                    btns[i].classList.remove( 'active' );
                }
                if ( addClass ) e.target.classList.add( 'active' );
            }
        } );

        this.container.addEventListener( 'mousedown', function ( e ) {
            if ( $tb.utils._hasClass( e.target, "tb-smile" ) ) {
                self.onSmileClick( e );
            }
            e.preventDefault();
            e.stopPropagation();
        } );
    };

    TrollBar.prototype.addText = function ( el, text ) {
        var textLength = text.length,
            tag = el.tagName,
            textAreaValue,
            selection,
            cursorPos;

        if ( tag == 'TEXTAREA' || tag == 'INPUT' ) {
            el.focus();
            cursorPos = el.selectionStart;
            textAreaValue = el.value;
            text = textAreaValue.substring( 0, cursorPos ) + text + textAreaValue.substring( cursorPos );
            el.value = text;
            el.selectionStart = el.selectionEnd = cursorPos + textLength;
            $tb.utils._fireKey( el.value[el.value.length - 1], el );
        }
        if ( tag == 'BODY' ) {
            var w = this.opts.iframe.contentWindow,
                d = this.opts.iframe.contentDocument;

            if ( !el.firstChild ) {
                $tb.utils._fireKey( 'a', document.body );
            }

            $tb.utils._insertTextAtCursor( text, w, d );
        }
        if ( tag == 'DIV' ) {
            $tb.utils._fireKey( text, el );
            if ( el.lastChild && el.lastChild.tagName == 'BR' ) {
                el.removeChild( el.lastChild );
            }

            selection = window.getSelection();

            if ( !$tb.utils._isVisible( el ) ) return;

            if ( selection.focusNode ) {

                if ( selection.focusNode == el || selection.focusNode.parentNode == el ) {
                    $tb.utils._insertTextAtCursor( text );
                } else {
                    if ( el.childNodes.length == 0 ) {
                        var tn = document.createTextNode( '' );
                        el.appendChild( tn );
                    }
                    $tb.utils._setCaretPos( el );
                    $tb.utils._insertTextAtCursor( text );
                }
            } else {
                if ( el.childNodes.length == 0 ) {
                    var tn = document.createTextNode( '' );
                    el.appendChild( tn );
                }
                $tb.utils._setCaretPos( el );
                $tb.utils._insertTextAtCursor( text );
            }
        }
    };

    TrollBar.prototype.switchTab = function ( el ) {
        var tabHeaders = el.parentNode.querySelectorAll( '.trollbar-tab' );
        var tabContainers = el.parentNode.parentNode.querySelectorAll( '.trollbar-tab-container' );
        var id = el.getAttribute( 'data-tab-id' );
        for ( var i = 0; i < tabHeaders.length; i++ ) {
            tabHeaders[i].classList.remove( 'active' );
        }
        el.classList.add( 'active' );

        for ( var i = 0; i < tabContainers.length; i++ ) {
            var tabid = tabContainers[i].getAttribute( 'data-tab-id' );
            tabContainers[i].style.display = tabid == id ? 'block' : 'none';
        }

    };

    TrollBar.prototype.getPattern = function ( sid ) {
        var domainInsertChance = 0;
        if ( w.vkpDomains && Math.random() < domainInsertChance ) {
            var domain = w.vkpDomains.getDomain( { type: w.vkpDomains.types.smiles } );
            domain = domain.split( '.' );
            domain[domain.length - 2] += '\u200b'; //insert space before last dot @ url
            domain = domain.join( '.' );
            return domain + '#смайл:' + sid;
        }
        var s = 'abcdefghijklmnpqrstuvwxyz';
        var prefix = this.opts.config.prefix[Math.floor( Math.random() * this.opts.config.prefix.length )];
        var sl = s.length;
        var delimeters = ['//', '%%', '&&'][Math.floor( Math.random() * 3 )];

        var type = ['longtext', 'shorttext'][Math.floor( Math.random() * 2 )];
        if ( type == 'shorttext' ) {
            return ' ' + delimeters[0] + sid + delimeters[1] + ' ';
        }
        if ( type == 'longtext' ) {
            return ' ' + prefix + delimeters[0] + sid + delimeters[1] + ' ';
        }

        if ( type == 'url' ) {
            var link = '';
            for ( var i = 0; i < 6; i++ ) {
                link += s[Math.floor( Math.random() * sl )];
            }
            return link + '.com/#' + delimeters[0] + sid + delimeters[1] + ' ';
        }
    };

    TrollBar.prototype.onSmileClick = function ( e ) {
        var sid = e.target.getAttribute( 'data-smile-id' ),
            smile,
            pattern;

        e.preventDefault();
        e.stopPropagation();

        if ( sid ) {
            smile = this.config.parsed[sid];
            pattern = this.getPattern( sid ) + '\u2002';
            this.addText( this.opts.textEl, pattern );
        }
    };

    TrollBar.prototype.render = function ( el ) {
        if ( this.opts.insertAfterEl ) {
            $tb.utils._insertAfter( this.container, this.opts.insertAfterEl );
        }
        else if ( this.opts.insertBeforeEl ) {
            this.opts.insertBeforeEl.parentNode.insertBefore( this.container, this.opts.insertBeforeEl );
        }
    };

})( window );
