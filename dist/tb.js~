    //        opts.el = trollBarBtn;
    //        opts.dir = 'ne';
    //        return opts;
    //    } );

        checker.addSelector( '#ba_post', function ( el, opts ) {
            opts.textEl = el.querySelector( '#ba_text' );
            var trollBarBtn = $tb.main.defaultBtnEl( {
                cssFloat   : 'right',
                position   : 'relative',
                marginRight: '20px'
            } );
            opts.insertAfterEl = el.querySelector( '#ba_add_media' );
            opts.el = trollBarBtn;
            opts.dir = 'n';
            return opts;
        } );

        checker.addSelector( '#pv_your_comment', function ( el, opts ) {
            opts.textEl = el.querySelector( '#pv_comment' );
            var trollBarBtn = $tb.main.defaultBtnEl( {
                position: 'relative',
                cssFloat: 'right'
            } );
            opts.insertAfterEl = el.querySelector( '.button_blue' );
            opts.el = trollBarBtn;
            opts.dir = 'n';
            return opts;
        } );

        checker.addSelector( '.fc_tab_txt', function ( el, opts ) {
            opts.textEl = el.querySelector( '.text' );
            if ( !opts.textEl ) return false;
            var trollBarBtn = $tb.main.defaultBtnEl( {
                position: 'absolute',
                right   : '25px',
                bottom  : '65px'
            } );
            opts.insertAfterEl = el.firstChild;
            opts.el = trollBarBtn;
            return opts;
        } );

        checker.addSelector( '#fwr_form', function ( el, opts ) {
            opts.textEl = el.querySelector( '#fwr_text' );
            if ( !opts.textEl ) return false;
            var trollBarBtn = $tb.main.defaultBtnEl( {
                position: 'relative',
                cssFloat: 'right'
            } );
            opts.insertAfterEl = el.querySelector( '.button_blue' );
            opts.el = trollBarBtn;
            return opts;
        } );

        //VK Wall comment
        checker.addSelector( '.reply_form', function ( el, opts ) {
            opts.textEl = el.querySelector( '.reply_field' );
            if ( !opts.textEl ) return false;
            var trollBarBtn = $tb.main.defaultBtnEl( {
                cssFloat: 'left',
                right   : '10px'
            } );
            opts.insertAfterEl = el.querySelector( '.reply_button_wrap' );
            opts.el = trollBarBtn;
            opts.dir = 'w';
            return opts;
        } );

        checker.check();

        var replacer = new $tb.Replacer( {
            config: res
        } );

        setTimeout( function () {
            var body = document.querySelector( 'body' );
            if ( body ) {
                replacer.replace( body );
            } else {
                setTimeout( arguments.callee, 20 );
            }
        }, 20 );

        document.addEventListener( 'DOMNodeInserted', function ( e ) {
            replacer.replace( e.target );
        } );
    } );
})
