    that.parseConfig = function ( c ) {
        var parsed = {};
        for ( var i in c.smiles ) {
            for ( var j in c.smiles[i] ) {
                parsed[j] = c.smiles[i][j];
                parsed[j].url = c.path + parsed[j].url;
            }
        }
        return parsed;
    };

    config.parsed = that.parseConfig( config );

    that.configUrl = 'http://scripts.trollbar.net/v1.0/smiles/config.json';

    that.loadConfig = function ( callback ) {
        var xhr = new XMLHttpRequest();
        xhr.open( "GET", that.configUrl + '?' + Math.random() );
        xhr.onreadystatechange = function () {
            if ( xhr.readyState == 4 ) {
                if ( xhr.status == 200 ) {
                    var config = JSON.parse( xhr.responseText );
                    config.parsed = that.parseConfig( config );
                    return callback( null, config );
                } else {
                    return callback( xhr.status );
                }
//                callback( null, config );
            }
        };
        xhr.send( null );
    };

