    //        opts.dir = 'nw';
    //        opts.el = trollBarBtn;
    //        return opts;
    //    } );

        //VK New Message
        checker.addSelector( '#imw_text', function ( el, opts ) {
            var trollBarBtn = $tb.main.defaultBtnEl();
            var buttons = document.querySelector( '.imw_buttons' );
            opts.insertBeforeEl = buttons.firstChild;
            trollBarBtn.style.right = '190px';
            trollBarBtn.style.marginTop = '1px';
            opts.dir = 'nw';
            opts.el = trollBarBtn;
            return opts;
        } );

        //VK messenger (http://vk.com/im?media=&sel=46816717)
        if ( !options.showBar ) checker.addSelector( '.im_editable', function ( el, opts ) {
            var trollBarBtn = $tb.main.defaultBtnEl();
            trollBarBtn.style.right = '80px';
            trollBarBtn.style.bottom = '-32px';
            opts.el = trollBarBtn;
            opts.dir = 'n';
            return opts;
        } );

        //VK messenger BAR (http://vk.com/im?media=&sel=46816717)
        if ( options.showBar ) checker.addSelector( '.im_editable', function ( el, opts ) {
            opts.panelStyle = {
                position: 'absolute',
                width   : '346px',
                top     : '-45px'
            };
            document.querySelector( '#im_peer_controls' ).style.paddingTop = '50px';
            opts.dir = 'n';
            return opts;
        } );

    //    checker.addSelector( '.wall_text', function ( el, opts ) {
    //        opts.textEl = el.querySelector( '.mention_rich_ta' );
    //        if ( !opts.textEl ) return false;
    //        var trollBarBtn = $tb.main.defaultBtnEl({
    //        });
