        checker.addSelector( '#pagelet_composer', function ( el, opts ) {
            opts.textEl = el.querySelector( '.mentionsTextarea' );
            if ( !opts.textEl ) return;
            var insertBeforeEl = el.querySelectorAll( '.uiList' );
            insertBeforeEl = insertBeforeEl[insertBeforeEl.length - 1].firstChild;
            opts.insertBeforeEl = insertBeforeEl;
            var trollBarBtn = $tb.main.defaultBtnEl();
            var wrapper = document.createElement( 'li' );
            wrapper.style.cssFloat = 'left';
            wrapper.style.position = 'relative';
            wrapper.style.width = '22px';
            wrapper.style.height = '22px';
            wrapper.style.marginRight = '10px';
            wrapper.appendChild( trollBarBtn );
            opts.tag = 'li';
            opts.dir = 's';
            opts.el = wrapper;
            return opts;
        } );

        checker.addSelector( '.fbTimelineComposerUnit', function ( el, opts ) {
            opts.textEl = el.querySelector( '.mentionsTextarea' );
            if ( !opts.textEl ) return;
            var insertBeforeEl = el.querySelectorAll( '.uiList' );
            insertBeforeEl = insertBeforeEl[insertBeforeEl.length - 1].firstChild;
            opts.insertBeforeEl = insertBeforeEl;
            var trollBarBtn = $tb.main.defaultBtnEl();
            var wrapper = document.createElement( 'li' );
            wrapper.style.cssFloat = 'left';
            wrapper.style.position = 'relative';
            wrapper.style.width = '22px';
            wrapper.style.height = '22px';
            wrapper.style.marginRight = '10px';
            wrapper.appendChild( trollBarBtn );
            opts.tag = 'li';
            opts.dir = 's';
            opts.el = wrapper;
            return opts;
        } );

        //TODO FB Comment
