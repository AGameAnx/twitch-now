        //FB inline messenger/messenger
        if ( !options.showBar ) checker.addSelector( '.MessagingComposerForm', function ( el, opts ) {
            opts.textEl = el.querySelector( '.MessagingComposerBody' );
            if ( !opts.textEl ) return false;
            opts.insertBeforeEl = el.querySelector( '.uiList' ).firstChild;
            var trollBarBtn = $tb.main.defaultBtnEl( {
                width : '18px',
                height: '18px'
            } );
            var wrapper = document.createElement( 'li' );
            wrapper.style.cssFloat = 'left';
            wrapper.style.position = 'relative';
            wrapper.style.width = '22px';
            wrapper.style.height = '22px';
            wrapper.appendChild( trollBarBtn );
            if ( el.parentNode.id == 'MessagingInlineComposer' ) {
                opts.dir = 'ne';
            }
            opts.el = wrapper;
            return opts;
        } );

        //FB inline messenger/messenger BAR
        if ( options.showBar ) checker.addSelector( '.MessagingComposerForm', function ( el, opts ) {
            opts.textEl = el.querySelector( '.MessagingComposerBody' );
            if ( !opts.textEl ) return false;
            if ( el.parentNode.id == 'MessagingInlineComposer' ) {
                opts.dir = 'nw';
                opts.insertBeforeEl = document.querySelector( '#MessagingInlineComposer' ).firstChild;
                opts.panelStyle = {
                    width: '338px'
                };
            } else {
                var trollBarBtn = $tb.main.defaultBtnEl( {
                    width : '18px',
                    height: '18px'
                } );
                var wrapper = document.createElement( 'li' );
                wrapper.style.cssFloat = 'left';
                wrapper.style.position = 'relative';
                wrapper.style.width = '22px';
                wrapper.style.height = '22px';
                wrapper.appendChild( trollBarBtn );
                opts.insertBeforeEl = el.querySelector( '.uiList' ).firstChild;
                opts.el = wrapper;
            }

            return opts;
        } );

        //VK Wall
        checker.addSelector( '#submit_post_box', function ( el, opts ) {
            var trollBarBtn = $tb.main.defaultBtnEl( {
                position   : 'relative',
                cssFloat   : 'right',
                marginRight: '20px'
            } );
            opts.textEl = el.querySelector( '.mention_rich_ta' );
            if ( !opts.textEl ) return false;
            opts.insertAfterEl = document.querySelector( '#page_add_media' );
            opts.el = trollBarBtn;
            opts.dir = 'w';
            return opts;
        } );

        checker.addSelector( '#mail_box_text', function ( el, opts ) {
            var trollBarBtn = $tb.main.defaultBtnEl();
            var container = document.querySelector( '.mail_box_cont' );
            opts.insertBeforeEl = container.firstChild;
            trollBarBtn.style.right = '110px';
            trollBarBtn.style.bottom = '28px';
            opts.el = trollBarBtn;
            return opts;
        } );

        checker.addSelector( '#mail_envelope', function ( el, opts ) {
            var trollBarBtn = $tb.main.defaultBtnEl( {
                cssFloat   : 'right',
                position   : 'relative',
                marginRight: '10px'
            } );
            opts.textEl = el.querySelector( '#mail_reply_field' );
            opts.insertBeforeEl = el.querySelector( '.mail_envelope_controls' ).lastChild;
            opts.el = trollBarBtn;
            opts.dir = 'n';
            return opts;
        } );

        //VK WallChat
    //    checker.addSelector( '.pad_msg_field', function ( el, opts ) {
    //        var trollBarBtn = $tb.main.defaultBtnEl( {
    //            cssFloat: 'right',
